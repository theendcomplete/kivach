# Mac specific files
.DS_Store
.idea
# Ignore bundler config.
/.bundle
.rails-env
/.vagrant
*.log

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

node_modules
/node_modules
/yarn-error.log

.byebug_history

lefthook-local.yml

